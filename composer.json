{
    "name" : "makinacorpus/filechunk-bundle",
    "type" : "symfony-bundle",
    "description" : "Javascript file upload widget and form type for Symfony",
    "homepage" : "http://github.com/makinacorpus/filechunk-bundle",
    "license" : "GPL-2.0-or-later",
    "authors" : [
        {
            "name" : "Pierre Rineau",
            "email" : "pierre.rineau@makina-corpus.com"
        },
        {
            "name" : "Makina Corpus",
            "homepage" : "http://makina-corpus.com"
        }
    ],
    "minimum-stability" : "dev",
    "prefer-stable" : true,
    "require": {
        "php": ">=8.0",
        "makinacorpus/files": "^1.0.1",
        "symfony/dependency-injection": "^5.4|^6.0",
        "symfony/event-dispatcher": "^5.4|^6.0",
        "symfony/framework-bundle": "^5.4|^6.0",
        "symfony/mime": "^5.4|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "MakinaCorpus\\FilechunkBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MakinaCorpus\\FilechunkBundle\\Tests\\": "tests/"
        }
    }
}
